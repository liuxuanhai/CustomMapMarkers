<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ender.mapfragment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="default_latitude"
            android:value="37.9922399" />
        <meta-data
            android:name="default_longitude"
            android:value="-1.1306544" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBLMmQdlwILw0zGQTU3uNi7QegbnWNdo8c" />

        <activity android:name=".view.MapActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>
